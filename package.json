    "@fortawesome/fontawesome-free": "^5.13.0",
    "font-awesome": "^4.7.0",
    "lite-server": "^2.3.0",
    "node-sass": "^4.14.0",
    "onchange": "^3.3.0",
    "parallelshell": "^3.0.1"
  },
  "dependencies": {
    "bootstrap": "^4.3.1",
    "jquery": "^3.3.1"
  }
}
