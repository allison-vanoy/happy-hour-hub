{
  "name": "happy-hour-hub",
  "version": "1.0.0",
  "description": "Website to add and view local happy hour deals.",
  "main": "index.html",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm run watch:all",
    "lite": "lite-server",
    "scss": "node-sass -o css/ css/",
